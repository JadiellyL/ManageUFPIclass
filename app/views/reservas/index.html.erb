<p id="notice"><%= notice %></p>

      <h2 class="text-center text-lg text-uppercase my-0"><strong> Reservas </strong></h2>
          <br>
          <br>

          <div class="container-fluid">

            <table>
            <thead>
              <tr>
                <th>Student</th>
                <th>Sala</th>
                <th>Time input</th>
                <th>Time output</th>
                <th>Date</th>
                <th>Quantity</th>
                <th colspan="3"></th>
              </tr>
            </thead>

            <tbody>
              <% @reservas.each do |reserva| %>
                <tr>
                  <td><%= reserva.student.name %></td>
                  <td><%= reserva.sala.number %></td>
                  <td><%= reserva.time_input %></td>
                  <td><%= reserva.time_output %></td>
                  <td><%= reserva.date %></td>
                  <td><%= reserva.quantity %></td>
                  <td><%= link_to 'Show', reserva %></td>
                  <td><%= link_to 'Edit', edit_reserva_path(reserva) %></td>
                  <td><%= link_to 'Destroy', reserva, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                </tr>
              <% end %>
            </tbody>
          </table>

          </div>

<br>
